cat .ssh/authorized_keys | vagrant ssh -- tee -a .ssh/authorized_keys

accept github.com, localhost ssh host keys

git clone git@github.com:defn/home

mv home/.git .
rm -rf home
git checkout .
rm -f .profile

for a in sub app jq pkgsrc python ansible aws gcloud letsencrypt erlang elixir \
         phoenix ruby rocco tugboat node dnsimple cue junas defn runmany deploy \
         virtualbox packer vagrant ubuntu boxcar; do 
  git clone git@github.com:defn/app-$a work/$a || git clone git@github.com:defn/$a work/$a || true
  (cd work/$a && git pull)
done

(cd work/jq && script/bootstrap)
script/update
source .bash_profile
app bootstrap
require
