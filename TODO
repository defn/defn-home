cat .ssh/authorized_keys | vagrant ssh -- tee -a .ssh/authorized_keys

ssh -t spiral git clone git@github.com:defn/home home
ssh spiral mv home/.git .
ssh spiral git reset --hard
ssh spiral rm -rf home
ssh spiral git clean -fd

rsync -ia --progress packages distfiles cache spiral:
ssh spiral sudo chown -R ubuntu:ubuntu progress packages distfiles cache

ssh -t spiral script/bootstrap
ssh -t spiral app bootstrap
