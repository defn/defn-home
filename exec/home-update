#!/usr/bin/env bash

function main {
  local shome="${_defn_home_home:-"$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"}"
  source "$shome/script/profile"
  source normalize  

  local many="${1:-10}"
  
  unset GIT_SSH ssh_gateway ssh_gateway_user

  block runmany "$many" 'cd $1 && git pull' || true
  block clone
  block lock
  home status
}

source sub "$BASH_SOURCE" "$@"
