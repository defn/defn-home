#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile"
  source normalize  

  local many="${1:-10}"
  
  unset GIT_SSH ssh_gateway ssh_gateway_user

  cache git update
  block runmany "$many" 'cd $1 && cache pull' || true
  block clone
  block lock
  home status
}

source sub "$BASH_SOURCE" "$@"
