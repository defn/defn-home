#!/usr/bin/env bash

function profile {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  PATH="$shome/bin:$shome/exec:$shome/node_modules/.bin:$PATH"

  source "$shome/work/app/script/profile"
  source "$APP_PATH/sub/script/profile"
  source "$APP_PATH/jq/script/profile"

  source app_strict

  source home_terminal
  source home_cache
  source home_secret 2>&1 >/dev/null || true
}

profile
