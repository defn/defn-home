#!/usr/bin/env bash

function profile {
  local shome="$(unset CDPATH; cd -P -- "$(dirname -- "$BASH_SOURCE")/.." && pwd -P)"
  PATH="$shome/bin:$shome/exec:$PATH"

  source app_strict
  source app_shortcuts
  source app_debug

  require python ansible aws gcloud letsencrypt
  require erlang elixir
  require ruby rocco tugboat
  require node dnsimple
  require go hub
  require jq
  require cue junas defn
  require runmany deploy boxcar
  require scripts-to-rule-them-all
  require vagrant packer
}

if ! profile; then
  true
fi

set +efu
