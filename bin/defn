#!/usr/bin/env bash

function args {
  source shflags

  DEFINE_string "name" "" "whatever"
}

function main {
  local shome="${_defn_home_home:-"$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"}"
  source "$shome/script/profile"

  export CONTEXT_ORG="${0##*/}"

  exec dev us-east-1 "$@"
}

source sub "$BASH_SOURCE" "$@"
